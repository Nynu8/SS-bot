    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/luxon": "^3.3.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.3",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "aws-lambda": "^1.0.7",
    "esbuild": "^0.17.15",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^46.0.0",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "inquirer-directory": "^2.2.0",
    "mocha": "^10.2.0",
    "npm-audit-resolver": "^3.0.0-RC.0",
    "plop": "^3.1.2",
    "prettier": "^2.8.4",
    "serverless": "^3.28.1",
    "serverless-esbuild": "^1.43.0",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-log-retention": "^2.0.0",
    "serverless-plugin-typescript": "^2.1.4",
    "sinon": "^15.0.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.382.0",
    "@aws-sdk/client-sqs": "^3.385.0",
    "@aws-sdk/lib-dynamodb": "^3.382.0",
    "@middy/core": "^4.2.7",
    "@middy/http-error-handler": "^4.2.7",
    "@middy/http-event-normalizer": "^4.2.7",
    "@middy/http-header-normalizer": "^4.2.7",
