{
  "name": "ss-bot",
  "version": "0.0.1",
  "description": "Various SS bot",
  "author": "Nynu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thisisxpate/ss-bot.git"
  },
  "bugs": {
    "url": "https://github.com/thisisxpate/ss-bot/issues"
  },
  "scripts": {
    "dev": "./node_modules/.bin/serverless offline start -s dev --httpPort 1337 --lambdaPort 4000 --reloadHandler",
    "test": "DOTENV_CONFIG_PATH=.env.test ./node_modules/.bin/mocha -r esbuild-runner/register -r dotenv/config \"./shared/**/*.spec.ts\" \"./functions/**/*.spec.ts\" --exit",
    "format": "./node_modules/.bin/prettier --write .",
    "check-types": "tsc --noemit",
    "lint": "./node_modules/.bin/eslint -c .eslintrc.json --ext .ts functions/ --ext .ts shared/",
    "lint-fix": "./node_modules/.bin/eslint -c .eslintrc.json --ext .ts --fix functions/ --ext .ts --fix shared/",
    "plop": "plop",
    "prepare": "husky install",
    "deploy": "./node_modules/.bin/serverless deploy",
    "invoke-local": "./node_modules/.bin/serverless invoke local --function",
    "resolve-audit": "./node_modules/.bin/resolve-audit",
    "audit": "./node_modules/.bin/check-audit"
  },
  "devDependencies": {
    "@babel/core": "7.21.3",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
